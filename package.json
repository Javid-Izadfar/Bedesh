{
  "name": "bedesh",
  "version": "1.0.0",
  "description": "Get user's email through modal",
  "main": "index.js",
  "scripts": {
    "build-css": "catw -c 'lessc -' 'src/bedesh.less' > dist/bedesh.css && postcss dist/bedesh.css --use autoprefixer -r --no-map",
    "build-js": "browserify src/bedesh.js -s Bedesh | ccjs - > dist/bedesh.js",
    "build": "npm run build-js && npm run build-css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Javid-Izadfar/Bedesh.git"
  },
  "keywords": [
    "Iran",
    "Email",
    "Farsi",
    "Modal"
  ],
  "author": "Javid Izadfar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Javid-Izadfar/Bedesh/issues"
  },
  "homepage": "https://github.com/Javid-Izadfar/Bedesh#readme",
  "dependencies": {
    "component-query": "^0.0.3",
    "cookie-cutter": "^0.1.1",
    "email-validator": "^1.0.7",
    "get-doc": "^1.0.3",
    "xtend": "^4.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "browserify": "^10.2.4",
    "catw": "^1.0.1",
    "closurecompiler": "^1.5.1",
    "less": "^2.7.2",
    "postcss-cli": "^3.0.0"
  }
}
